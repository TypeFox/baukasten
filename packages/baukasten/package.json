{
  "name": "baukasten-ui",
  "version": "0.1.0",
  "description": "A UI toolkit for Web, Eclipse Theia and VSCode",
  "homepage": "https://typefox.dev/baukasten",
  "keywords": [
    "baukasten-ui",
    "ui",
    "toolkit",
    "vscode",
    "Theia",
    "react",
    "components"
  ],
  "license": "MIT",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./styles": {
      "import": "./dist/styles.mjs",
      "require": "./dist/styles.js"
    },
    "./dist/style.css": "./dist/style.css"
  },
  "files": [
    "dist",
    "src"
  ],
  "sideEffects": false,
  "scripts": {
    "dev": "vite",
    "build": "tsc --project tsconfig.build.json && vite build",
    "preview": "vite preview",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:watch": "vitest --watch",
    "test:coverage": "vitest --coverage"
  },
  "peerDependencies": {
    "react": "^19.0.0",
    "react-dom": "^19.0.0"
  },
  "dependencies": {
    "@floating-ui/react": "^0.27.16",
    "@tanstack/react-table": "^8.21.3",
    "@vscode/codicons": "^0.0.41"
  },
  "devDependencies": {
    "baukasten-ui-web-wrapper": "*",
    "@storybook/addon-essentials": "^8.4.0",
    "@storybook/addon-interactions": "^8.4.0",
    "@storybook/addon-links": "^8.4.0",
    "@storybook/blocks": "^8.4.0",
    "@storybook/react": "^8.4.0",
    "@storybook/react-vite": "^8.4.0",
    "@storybook/test": "^8.4.0",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/react": "^19.0.0",
    "@types/react-dom": "^19.0.0",
    "@typescript-eslint/eslint-plugin": "^6.18.1",
    "@typescript-eslint/parser": "^6.18.1",
    "@vanilla-extract/css": "^1.17.4",
    "@vanilla-extract/recipes": "^0.5.7",
    "@vanilla-extract/vite-plugin": "^5.1.3",
    "@vitejs/plugin-react": "^4.2.1",
    "@vitest/coverage-v8": "^4.0.1",
    "eslint": "^8.56.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "eslint-plugin-storybook": "^0.6.15",
    "jsdom": "^27.0.1",
    "react": "^19.0.0",
    "react-docgen": "^7.1.0",
    "react-dom": "^19.0.0",
    "storybook": "^8.4.0",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.3.3",
    "vite": "^6.0.0",
    "vite-plugin-dts": "^4.5.4",
    "vitest": "^4.0.1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/typefox/baukasten.git",
    "directory": "packages/baukasten"
  },
  "bugs": "https://github.com/typefox/baukasten/issues",
  "author": {
    "name": "TypeFox",
    "url": "https://www.typefox.io"
  }
}